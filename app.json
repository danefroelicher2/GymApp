{
  "expo": {
    "name": "GymTracker",
    "slug": "gym-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/48x48.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/1284x2778.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.gymtracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/1284x2778.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourcompany.gymtracker"
    },
    "web": {
      "favicon": "./assets/48x48.png"
    },
    "scheme": "gymtracker",
    "plugins": ["expo-secure-store"]
  }
}
